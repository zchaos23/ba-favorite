/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "isRage", function() { return isRage; });
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "isDead", function() { return isDead; });
/* harmony import */ var _cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__ = __webpack_require__(/*! @cloudcare/browser-core */ "./node_modules/.pnpm/@cloudcare+browser-core@3.1.1/node_modules/@cloudcare/browser-core/esm/index.js");

var MIN_CLICKS_PER_SECOND_TO_CONSIDER_RAGE = 3;
function computeFrustration(clicks, rageClick) {
  if (isRage(clicks)) {
    rageClick.addFrustration(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["FrustrationType"].RAGE_CLICK);
    if (Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["some"])(clicks, isDead)) {
      rageClick.addFrustration(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["FrustrationType"].DEAD_CLICK);
    }
    if (rageClick.hasError()) {
      rageClick.addFrustration(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["FrustrationType"].ERROR_CLICK);
    }
    return {
      isRage: true
    };
  }
  var hasSelectionChanged = Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["some"])(clicks, function (click) {
    return click.getUserActivity().selection;
  });
  Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["each"])(clicks, function (click) {
    if (click.hasError()) {
      click.addFrustration(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["FrustrationType"].ERROR_CLICK);
    }
    if (isDead(click) &&
    // Avoid considering clicks part of a double-click or triple-click selections as dead clicks
    !hasSelectionChanged) {
      click.addFrustration(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["FrustrationType"].DEAD_CLICK);
    }
  });
  return {
    isRage: false
  };
}
function isRage(clicks) {
  if (Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["some"])(clicks, function (click) {
    return click.getUserActivity().selection;
  })) {
    return false;
  }
  for (var i = 0; i < clicks.length - (MIN_CLICKS_PER_SECOND_TO_CONSIDER_RAGE - 1); i += 1) {
    if (clicks[i + MIN_CLICKS_PER_SECOND_TO_CONSIDER_RAGE - 1].event.timeStamp - clicks[i].event.timeStamp <= _cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["ONE_SECOND"]) {
      return true;
    }
  }
  return false;
}
var DEAD_CLICK_EXCLUDE_SELECTOR =
// inputs that don't trigger a meaningful event like "input" when clicked, including textual
// inputs (using a negative selector is shorter here)
'input:not([type="checkbox"]):not([type="radio"]):not([type="button"]):not([type="submit"]):not([type="reset"]):not([type="range"]),' + 'textarea,' + 'select,' +
// contenteditable and their descendants don't always trigger meaningful changes when manipulated
'[contenteditable],' + '[contenteditable] *,' +
// canvas, as there is no good way to detect activity occurring on them
'canvas,' +
// links that are interactive (have an href attribute) or any of their descendants, as they can
// open a new tab or navigate to a hash without triggering a meaningful event
'a[href],' + 'a[href] *';
function isDead(click) {
  if (click.hasPageActivity() || click.getUserActivity().input) {
    return false;
  }
  return !Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["elementMatches"])(click.event.target, DEAD_CLICK_EXCLUDE_SELECTOR);
}
