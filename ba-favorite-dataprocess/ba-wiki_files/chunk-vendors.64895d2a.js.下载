  var viewEvent = {
    _gc: {
      document_version: view.documentVersion,
      replay_stats: replayStats,
      page_states: pageStates
    },
    date: view.startClocks.timeStamp,
    type: _cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["RumEventType"].VIEW,
    view: {
      action: {
