/*! exports provided: MAX_ATTRIBUTE_VALUE_CHAR_LENGTH, getNodePrivacyLevel, reducePrivacyLevel, getNodeSelfPrivacyLevel, shouldMaskNode, censorText, getTextContent, shouldIgnoreElement */
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "MAX_ATTRIBUTE_VALUE_CHAR_LENGTH", function() { return MAX_ATTRIBUTE_VALUE_CHAR_LENGTH; });
