  var observable = new _cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["Observable"](function () {
    var _trackHistory = trackHistory(onLocationChange);
    var _trackHash = trackHash(onLocationChange);
    return function () {
      _trackHistory.stop();
      _trackHash.stop();
    };
  });
  function onLocationChange() {
    if (currentLocation.href === location.href) {
      return;
    }
    var newLocation = Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["shallowClone"])(location);
    observable.notify({
      newLocation: newLocation,
      oldLocation: currentLocation
    });
    currentLocation = newLocation;
  }
  return observable;
}
