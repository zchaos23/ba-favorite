/**
 * We want to attach to an event:
 * - the url corresponding to its start
 * - the referrer corresponding to the previous view url (or document referrer for initial view)
 */

var URL_CONTEXT_TIME_OUT_DELAY = _cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["SESSION_TIME_OUT_DELAY"];
function startUrlContexts(lifeCycle, locationChangeObservable, location) {
  var urlContextHistory = new _cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["ContextHistory"](URL_CONTEXT_TIME_OUT_DELAY);
  var previousViewUrl;
  lifeCycle.subscribe(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["LifeCycleEventType"].VIEW_ENDED, function (data) {
    urlContextHistory.closeActive(data.endClocks.relative);
  });
  lifeCycle.subscribe(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["LifeCycleEventType"].VIEW_CREATED, function (data) {
    var viewUrl = location.href;
    urlContextHistory.add(buildUrlContext({
      url: viewUrl,
      location: location,
      referrer: !previousViewUrl ? document.referrer : previousViewUrl
    }), data.startClocks.relative);
    previousViewUrl = viewUrl;
  });
  var locationChangeSubscription = locationChangeObservable.subscribe(function (data) {
    var current = urlContextHistory.find();
    if (current) {
      var changeTime = Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["relativeNow"])();
      urlContextHistory.closeActive(changeTime);
      urlContextHistory.add(buildUrlContext({
        url: data.newLocation.href,
        location: data.newLocation,
        referrer: current.referrer
      }), changeTime);
    }
  });
  function buildUrlContext(data) {
    var path = data.location.pathname;
    var hash = data.location.hash;
    if (path === '/' && hash && !Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["isHashAnAnchor"])(hash)) {
      path = '/' + Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["getPathFromHash"])(hash);
    }
    return {
      url: data.url,
      referrer: data.referrer,
      host: data.location.host,
      path: path,
      pathGroup: Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["replaceNumberCharByPath"])(path),
      urlQuery: Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["jsonStringify"])(Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["getQueryParamsFromUrl"])(data.location.href))
    };
  }
  return {
    findUrl: function findUrl(startTime) {
      return urlContextHistory.find(startTime);
    },
    stop: function stop() {
      locationChangeSubscription.unsubscribe();
      urlContextHistory.stop();
    }
  };
}
