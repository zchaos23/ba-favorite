    var _this = this;
    var u = VariableLibrary.navigator.userAgent || '';
    _this.osVersion = '';
    _this.osMajor = '';
    // 系统版本信息
    var osVersion = {
      Windows: function Windows() {
        var v = u.replace(/^.*Windows NT ([\d.]+);.*$/, '$1');
        var oldWindowsVersionMap = {
          6.4: '10',
          6.3: '8.1',
          6.2: '8',
          6.1: '7',
          '6.0': 'Vista',
          5.2: 'XP',
          5.1: 'XP',
          '5.0': '2000'
        };
