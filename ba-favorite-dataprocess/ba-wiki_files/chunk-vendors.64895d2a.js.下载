        type: _cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["ActionType"].CLICK,
        duration: activityEndTime && Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["elapsed"])(startClocks.timeStamp, activityEndTime),
        startClocks: startClocks,
        id: id,
        frustrationTypes: frustrationTypes,
        counts: {
          resourceCount: resourceCount,
          errorCount: errorCount,
          longTaskCount: longTaskCount
        },
