      status: DeflateWorkerStatus.Error
    };
  } else {
    Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["addTelemetryError"])(error, {
      worker_version: state.status === DeflateWorkerStatus.Initialized && state.version,
      stream_id: streamId
