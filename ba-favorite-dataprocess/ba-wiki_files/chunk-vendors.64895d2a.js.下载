function computePerformanceViewDetails(entry) {
  var validEntry = Object(_resource_resourceUtils__WEBPACK_IMPORTED_MODULE_2__["toValidEntry"])(entry);
  if (!validEntry) {
    return undefined;
  }
  var fetchStart = validEntry.fetchStart,
    responseEnd = validEntry.responseEnd,
    domInteractive = validEntry.domInteractive,
    domContentLoaded = validEntry.domContentLoaded,
    domComplete = validEntry.domComplete,
    loadEventEnd = validEntry.loadEventEnd,
    loadEventStart = validEntry.loadEventStart,
    domContentLoadedEventEnd = validEntry.domContentLoadedEventEnd;
  var details = {};
  if (responseEnd !== fetchStart) {
    details.fpt = Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["toServerDuration"])(responseEnd - fetchStart);
    var apdexLevel = parseInt((responseEnd - fetchStart) / 1000); // 秒数取整
    details.apdexLevel = apdexLevel > 9 ? 9 : apdexLevel;
  }
  if (domInteractive !== fetchStart) {
    details.tti = Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["toServerDuration"])(domInteractive - fetchStart);
  }
  if (domContentLoaded !== fetchStart) {
    details.dom_ready = Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["toServerDuration"])(domContentLoaded - fetchStart);
  }
  // Make sure a connection occurred
  if (loadEventEnd !== fetchStart) {
    details.load = Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["toServerDuration"])(loadEventEnd - fetchStart);
  }
  if (loadEventStart !== domContentLoadedEventEnd) {
    details.resource_load_time = Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["toServerDuration"])(loadEventStart - domContentLoadedEventEnd);
  }
  if (domComplete !== domInteractive) {
    details.dom = Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["toServerDuration"])(domComplete - domInteractive);
  }
  return details;
}
