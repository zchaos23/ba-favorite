  var currentClickChain;
  lifeCycle.subscribe(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["LifeCycleEventType"].SESSION_RENEWED, function () {
    history.reset();
  });
  lifeCycle.subscribe(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["LifeCycleEventType"].VIEW_ENDED, stopClickChain);
  var _listenActionEvents = Object(_listenActionEvents__WEBPACK_IMPORTED_MODULE_6__["listenActionEvents"])({
