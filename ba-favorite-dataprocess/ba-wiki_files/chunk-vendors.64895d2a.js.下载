  var sessionStore = Object(_sessionStore__WEBPACK_IMPORTED_MODULE_3__["startSessionStore"])(options, productKey, computeSessionState);
  stopCallbacks.push(function () {
    return sessionStore.stop();
  });
  var sessionContextHistory = new _helper_contextHistory__WEBPACK_IMPORTED_MODULE_2__["ContextHistory"](SESSION_CONTEXT_TIMEOUT_DELAY);
  stopCallbacks.push(function () {
    return sessionContextHistory.stop();
