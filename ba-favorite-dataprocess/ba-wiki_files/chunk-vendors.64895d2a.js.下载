  if (initConfiguration.tracingSampleRate !== undefined && !Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["isPercentage"])(initConfiguration.tracingSampleRate)) {
    _cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["display"].error('Tracing Sample Rate should be a number between 0 and 100');
    return;
  }
  if (initConfiguration.excludedActivityUrls !== undefined && !Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["isArray"])(initConfiguration.excludedActivityUrls)) {
    _cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["display"].error('Excluded Activity Urls should be an array');
    return;
  }
  var baseConfiguration = Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["validateAndBuildConfiguration"])(initConfiguration);
  if (!baseConfiguration) {
    return;
  }
  var trackUserInteractions = !!Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["isNullUndefinedDefaultValue"])(initConfiguration.trackUserInteractions, initConfiguration.trackInteractions);
