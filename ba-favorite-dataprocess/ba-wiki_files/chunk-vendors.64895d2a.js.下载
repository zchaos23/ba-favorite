    return {
      batch: batch,
      flushController: flushController
    };
  }
  return {
    flushObservable: primaryFlushController.flushObservable,
    add: function add(message) {
      primaryBatch.add(message);
    },
    upsert: function upsert(message, key) {
      primaryBatch.upsert(message, key);
    }
  };
}
