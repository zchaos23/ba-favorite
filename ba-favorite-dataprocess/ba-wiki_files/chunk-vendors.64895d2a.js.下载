/**
 * Handles allowedTracingUrls and processes legacy allowedTracingOrigins
 */
function validateAndBuildTracingOptions(initConfiguration) {
  // Advise about parameters precedence.
  if (initConfiguration.allowedTracingUrls !== undefined && initConfiguration.allowedTracingOrigins !== undefined) {
    _cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["display"].warn('Both allowedTracingUrls and allowedTracingOrigins (deprecated) have been defined. The parameter allowedTracingUrls will override allowedTracingOrigins.');
  }
  // Handle allowedTracingUrls first
  if (initConfiguration.allowedTracingUrls !== undefined) {
    if (!Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["isArray"])(initConfiguration.allowedTracingUrls)) {
      _cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["display"].error('Allowed Tracing URLs should be an array');
      return;
    }
    // if (initConfiguration.allowedTracingUrls.length !== 0 && initConfiguration.service === undefined) {
    //   display.error('Service needs to be configured when tracing is enabled')
    //   return
    // }
    // Convert from (MatchOption | TracingOption) to TracingOption, remove unknown properties
    var tracingOptions = [];
    Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["each"])(initConfiguration.allowedTracingUrls, function (option) {
      if (Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["isMatchOption"])(option)) {
        tracingOptions.push({
          match: option,
          traceType: Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["isNullUndefinedDefaultValue"])(initConfiguration.traceType, _cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["TraceType"].DDTRACE)
        });
      } else if (Object(_domain_tracing_tracer__WEBPACK_IMPORTED_MODULE_2__["isTracingOption"])(option)) {
        tracingOptions.push(option);
      } else {
        _cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["display"].warn('Allowed Tracing Urls parameters should be a string, RegExp, function, or an object. Ignoring parameter', option);
      }
    });
    return tracingOptions;
  }

  // Handle conversion of allowedTracingOrigins to allowedTracingUrls
  if (initConfiguration.allowedTracingOrigins !== undefined) {
    if (!Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["isArray"])(initConfiguration.allowedTracingOrigins)) {
      _cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["display"].error('Allowed Tracing Origins should be an array');
      return;
    }
    var tracingOptions = [];
    Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["each"])(initConfiguration.allowedTracingOrigins, function (legacyMatchOption) {
      var tracingOption = convertLegacyMatchOptionToTracingOption(legacyMatchOption, Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["isNullUndefinedDefaultValue"])(initConfiguration.traceType, _cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["TraceType"].DDTRACE));
      if (tracingOption) {
        tracingOptions.push(tracingOption);
      }
    });
    return tracingOptions;
  }
  // Handle conversion of allowedDDTracingOrigins to allowedTracingUrls
  if (initConfiguration.allowedDDTracingOrigins !== undefined) {
    if (!Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["isArray"])(initConfiguration.allowedDDTracingOrigins)) {
      _cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["display"].error('Allowed Tracing Origins should be an array');
      return;
    }
    var tracingOptions = [];
    Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["each"])(initConfiguration.allowedDDTracingOrigins, function (legacyMatchOption) {
      var tracingOption = convertLegacyMatchOptionToTracingOption(legacyMatchOption, Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["isNullUndefinedDefaultValue"])(initConfiguration.traceType, _cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["TraceType"].DDTRACE));
      if (tracingOption) {
        tracingOptions.push(tracingOption);
      }
    });
    return tracingOptions;
  }
  return [];
}

/**
 * Converts parameters from the deprecated allowedTracingOrigins
 * to allowedTracingUrls. Handles the change from origin to full URLs.
 */
function convertLegacyMatchOptionToTracingOption(item, traceType) {
  var match;
  if (typeof item === 'string') {
    match = item;
  } else if (item instanceof RegExp) {
    match = function match(url) {
      return item.test(Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["getOrigin"])(url));
    };
  } else if (typeof item === 'function') {
    match = function match(url) {
      return item(Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["getOrigin"])(url));
    };
  }
  if (match === undefined) {
    _cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["display"].warn('Allowed Tracing Origins parameters should be a string, RegExp or function. Ignoring parameter', item);
    return undefined;
  }
  return {
    match: match,
    traceType: traceType
  };
}
