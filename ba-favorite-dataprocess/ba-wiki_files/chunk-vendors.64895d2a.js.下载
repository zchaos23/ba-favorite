      if (entry.entryType === 'resource' && !Object(_resourceUtils__WEBPACK_IMPORTED_MODULE_2__["isRequestKind"])(entry) && !Object(_resourceUtils__WEBPACK_IMPORTED_MODULE_2__["isResourceUrlLimit"])(entry.name, configuration.resourceUrlLimit)) {
