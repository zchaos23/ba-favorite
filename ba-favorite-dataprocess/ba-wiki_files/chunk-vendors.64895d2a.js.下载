function supportPerformanceObject() {
  return window.performance !== undefined && 'getEntries' in performance;
}
function supportPerformanceTimingEvent(entryType) {
  return window.PerformanceObserver && PerformanceObserver.supportedEntryTypes !== undefined && Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["includes"])(PerformanceObserver.supportedEntryTypes, entryType);
}
function startPerformanceCollection(lifeCycle, configuration) {
  retrieveInitialDocumentResourceTiming(function (timing) {
    handleRumPerformanceEntries(lifeCycle, configuration, [timing]);
  });
  if (supportPerformanceObject()) {
    var performanceEntries = performance.getEntries();
    // Because the performance entry list can be quite large
    // delay the computation to prevent the SDK from blocking the main thread on init
    Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["setTimeout"])(function () {
      handleRumPerformanceEntries(lifeCycle, configuration, performanceEntries);
    });
  }
  if (window.PerformanceObserver) {
    var handlePerformanceEntryList = Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["monitor"])(function (entries) {
      handleRumPerformanceEntries(lifeCycle, configuration, entries.getEntries());
    });
    var mainEntries = ['resource', 'navigation', 'longtask', 'paint'];
    var experimentalEntries = ['largest-contentful-paint', 'first-input', 'layout-shift'];
    try {
      // Experimental entries are not retrieved by performance.getEntries()
      // use a single PerformanceObserver with buffered flag by type
      // to get values that could happen before SDK init
      Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["each"])(experimentalEntries, function (type) {
        var observer = new PerformanceObserver(handlePerformanceEntryList);
        observer.observe({
          type: type,
          buffered: true
        });
      });
    } catch (e) {
      // Some old browser versions (ex: chrome 67) don't support the PerformanceObserver type and buffered options
      // In these cases, fallback to PerformanceObserver with entryTypes
      Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["each"])(experimentalEntries, function (type) {
        mainEntries.push(type);
      });
    }
    var mainObserver = new PerformanceObserver(handlePerformanceEntryList);
    mainObserver.observe({
      entryTypes: mainEntries
    });
    if (supportPerformanceObject() && 'addEventListener' in performance) {
      // https://bugzilla.mozilla.org/show_bug.cgi?id=1559377
      Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["addEventListener"])(performance, 'resourcetimingbufferfull', function () {
        performance.clearResourceTimings();
      });
    }
  }
  if (!supportPerformanceTimingEvent('navigation')) {
    retrieveNavigationTiming(function (timing) {
      handleRumPerformanceEntries(lifeCycle, configuration, [timing]);
    });
  }
  if (!supportPerformanceTimingEvent('first-input')) {
    retrieveFirstInputTiming(function (timing) {
      return handleRumPerformanceEntries(lifeCycle, configuration, [timing]);
    });
  }
}
function retrieveInitialDocumentResourceTiming(callback) {
  Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["runOnReadyState"])('interactive', function () {
    var timing;
    var forcedAttributes = {
      entryType: 'resource',
      initiatorType: _rumEventsCollection_resource_resourceUtils__WEBPACK_IMPORTED_MODULE_1__["FAKE_INITIAL_DOCUMENT"],
      traceId: ''
    };
    if (supportPerformanceTimingEvent('navigation') && performance.getEntriesByType('navigation').length > 0) {
      var navigationEntry = performance.getEntriesByType('navigation')[0];
      timing = Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["extend"])(navigationEntry.toJSON(), forcedAttributes);
    } else {
      var relativePerformanceTiming = computeRelativePerformanceTiming();
      timing = Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["extend"])(relativePerformanceTiming, {
        decodedBodySize: 0,
        duration: relativePerformanceTiming.responseEnd,
        name: window.location.href,
        startTime: 0
      }, forcedAttributes);
    }
    callback(timing);
  });
}
function retrieveNavigationTiming(callback) {
  function sendFakeTiming() {
    callback(Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["extend"])(computeRelativePerformanceTiming(), {
      entryType: 'navigation'
    }));
  }
  Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["runOnReadyState"])('complete', function () {
    // Send it a bit after the actual load event, so the "loadEventEnd" timing is accurate
    Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["setTimeout"])(sendFakeTiming);
  });
}

/**
 * first-input timing entry polyfill based on
 * https://github.com/GoogleChrome/web-vitals/blob/master/src/lib/polyfills/firstInputPolyfill.ts
 */
function retrieveFirstInputTiming(callback) {
  var startTimeStamp = Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["dateNow"])();
  var timingSent = false;
  var listeners = Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["addEventListeners"])(window, [_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["DOM_EVENT"].CLICK, _cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["DOM_EVENT"].MOUSE_DOWN, _cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["DOM_EVENT"].KEY_DOWN, _cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["DOM_EVENT"].TOUCH_START, _cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["DOM_EVENT"].POINTER_DOWN], function (evt) {
    // Only count cancelable events, which should trigger behavior important to the user.
    if (!evt.cancelable) {
      return;
    }

    // This timing will be used to compute the "first Input delay", which is the delta between
    // when the system received the event (e.g. evt.timeStamp) and when it could run the callback
    // (e.g. performance.now()).
    var timing = {
      entryType: 'first-input',
      processingStart: Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["relativeNow"])(),
      startTime: evt.timeStamp
    };
    if (evt.type === _cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["DOM_EVENT"].POINTER_DOWN) {
      sendTimingIfPointerIsNotCancelled(timing);
    } else {
      sendTiming(timing);
    }
  }, {
    passive: true,
    capture: true
  });
  var removeEventListeners = listeners.stop;

  /**
   * Pointer events are a special case, because they can trigger main or compositor thread behavior.
   * We differenciate these cases based on whether or not we see a pointercancel event, which are
   * fired when we scroll. If we're scrolling we don't need to report input delay since FID excludes
   * scrolling and pinch/zooming.
   */
  function sendTimingIfPointerIsNotCancelled(timing) {
    Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["addEventListeners"])(window, [_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["DOM_EVENT"].POINTER_UP, _cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["DOM_EVENT"].POINTER_CANCEL], function (event) {
      if (event.type === _cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["DOM_EVENT"].POINTER_UP) {
        sendTiming(timing);
      }
    }, {
      once: true
    });
  }
  function sendTiming(timing) {
    if (!timingSent) {
      timingSent = true;
      removeEventListeners();
      // In some cases the recorded delay is clearly wrong, e.g. it's negative or it's larger than
      // the time between now and when the page was loaded.
      // - https://github.com/GoogleChromeLabs/first-input-delay/issues/4
      // - https://github.com/GoogleChromeLabs/first-input-delay/issues/6
      // - https://github.com/GoogleChromeLabs/first-input-delay/issues/7
      var delay = timing.processingStart - timing.startTime;
      if (delay >= 0 && delay < Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["dateNow"])() - startTimeStamp) {
        callback(timing);
      }
    }
  }
}
function computeRelativePerformanceTiming() {
  var result = {};
  var timing = performance.timing;
  for (var key in timing) {
    if (Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["isNumber"])(timing[key])) {
      result[key] = timing[key] === 0 ? 0 : Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["getRelativeTime"])(timing[key]);
    }
  }
  return result;
}
function handleRumPerformanceEntries(lifeCycle, configuration, entries) {
  var rumPerformanceEntries = Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["filter"])(entries, function (entry) {
    return entry.entryType === 'resource' || entry.entryType === 'navigation' || entry.entryType === 'paint' || entry.entryType === 'longtask' || entry.entryType === 'largest-contentful-paint' || entry.entryType === 'first-input' || entry.entryType === 'layout-shift';
  });
  var rumAllowedPerformanceEntries = Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["filter"])(rumPerformanceEntries, function (entry) {
    return !isIncompleteNavigation(entry) && !isForbiddenResource(configuration, entry);
  });
  if (rumAllowedPerformanceEntries.length) {
    lifeCycle.notify(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["LifeCycleEventType"].PERFORMANCE_ENTRIES_COLLECTED, rumAllowedPerformanceEntries);
  }
}
function isIncompleteNavigation(entry) {
  return entry.entryType === 'navigation' && entry.loadEventEnd <= 0;
}
function isForbiddenResource(configuration, entry) {
  return entry.entryType === 'resource' && !Object(_rumEventsCollection_resource_resourceUtils__WEBPACK_IMPORTED_MODULE_1__["isAllowedRequestUrl"])(configuration, entry.name);
}
function supportPerformanceEntry() {
  // Safari 10 doesn't support PerformanceEntry
  return typeof PerformanceEntry === 'function';
}
