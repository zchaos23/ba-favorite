function computeScrollValues() {
  var scrollTop = Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["getScrollY"])();
  var viewport = Object(_initViewportObservable__WEBPACK_IMPORTED_MODULE_1__["getViewportDimension"])();
