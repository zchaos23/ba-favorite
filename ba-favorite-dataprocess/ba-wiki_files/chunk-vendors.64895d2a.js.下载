    var endpoint = String(Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["base64Encode"])(window.location.href));
    var peer = String(Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["base64Encode"])(requestUrlHost));
    var index = '0';
    // var values = `${1}-${traceIdStr}-${segmentId}-${index}-${service}-${instance}-${endpoint}-${peer}`;
    return '1-' + traceIdStr + '-' + segmentId + '-' + index + '-' + service + '-' + instance + '-' + endpoint + '-' + peer;
  } catch (err) {
    return '';
  }
}
