/* harmony import */ var _helper_tools__WEBPACK_IMPORTED_MODULE_0__ = __webpack_require__(/*! ../helper/tools */ "./node_modules/.pnpm/@cloudcare+browser-core@3.1.1/node_modules/@cloudcare/browser-core/esm/helper/tools.js");
/* harmony import */ var _helper_byteUtils__WEBPACK_IMPORTED_MODULE_1__ = __webpack_require__(/*! ../helper/byteUtils */ "./node_modules/.pnpm/@cloudcare+browser-core@3.1.1/node_modules/@cloudcare/browser-core/esm/helper/byteUtils.js");
