  });
  lifeCycle.subscribe(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["LifeCycleEventType"].PERFORMANCE_ENTRIES_COLLECTED, function (entries) {
    for (var i = 0; i < entries.length; i++) {
