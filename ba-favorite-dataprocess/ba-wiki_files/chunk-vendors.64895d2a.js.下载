/* harmony import */ var _cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__ = __webpack_require__(/*! @cloudcare/browser-core */ "./node_modules/.pnpm/@cloudcare+browser-core@3.1.1/node_modules/@cloudcare/browser-core/esm/index.js");
/* harmony import */ var _performanceCollection__WEBPACK_IMPORTED_MODULE_1__ = __webpack_require__(/*! ../../performanceCollection */ "./node_modules/.pnpm/@cloudcare+browser-rum@3.1.1/node_modules/@cloudcare/browser-rum/esm/domain/performanceCollection.js");


/**
 * Track the cumulative layout shifts (CLS).
 * Layout shifts are grouped into session windows.
 * The minimum gap between session windows is 1 second.
 * The maximum duration of a session window is 5 second.
 * The session window layout shift value is the sum of layout shifts inside it.
 * The CLS value is the max of session windows values.
 *
 * This yields a new value whenever the CLS value is updated (a higher session window value is computed).
 *
 * See isLayoutShiftSupported to check for browser support.
 *
 * Documentation:
 * https://web.dev/cls/
 * https://web.dev/evolving-cls/
 * Reference implementation: https://github.com/GoogleChrome/web-vitals/blob/master/src/getCLS.ts
 */
function trackCumulativeLayoutShift(lifeCycle, callback) {
  var maxClsValue = 0;
  var window = slidingSessionWindow();
  var _subscribe = lifeCycle.subscribe(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["LifeCycleEventType"].PERFORMANCE_ENTRIES_COLLECTED, function (entries) {
    for (var i = 0; i < entries.length; i++) {
      var entry = entries[i];
      if (entry.entryType === 'layout-shift' && !entry.hadRecentInput) {
        window.update(entry);
        if (window.value() > maxClsValue) {
          maxClsValue = window.value();
          callback(Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["round"])(maxClsValue, 4));
        }
      }
    }
  });
  var stop = _subscribe.unsubscribe;
  return {
    stop: stop
  };
}
function slidingSessionWindow() {
  var _value = 0;
  var startTime;
  var endTime;
  return {
    update: function update(entry) {
      var shouldCreateNewWindow = startTime === undefined || entry.startTime - endTime >= _cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["ONE_SECOND"] || entry.startTime - startTime >= 5 * _cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["ONE_SECOND"];
      if (shouldCreateNewWindow) {
        startTime = endTime = entry.startTime;
        _value = entry.value;
      } else {
        _value += entry.value;
        endTime = entry.startTime;
      }
    },
    value: function value() {
      return _value;
    }
  };
}

/**
 * Check whether `layout-shift` is supported by the browser.
 */
function isLayoutShiftSupported() {
  return Object(_performanceCollection__WEBPACK_IMPORTED_MODULE_1__["supportPerformanceTimingEvent"])('layout-shift');
}
//# sourceMappingURL=trackCumulativeLayoutShift.js.map

