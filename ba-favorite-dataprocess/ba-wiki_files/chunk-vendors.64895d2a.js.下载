  sessionStore.expireObservable.subscribe(function () {
    sessionContextHistory.closeActive(Object(_helper_tools__WEBPACK_IMPORTED_MODULE_0__["relativeNow"])());
  });
  sessionStore.expandOrRenewSession();
  sessionContextHistory.add(buildSessionContext(), Object(_helper_tools__WEBPACK_IMPORTED_MODULE_0__["clocksOrigin"])().relative);
