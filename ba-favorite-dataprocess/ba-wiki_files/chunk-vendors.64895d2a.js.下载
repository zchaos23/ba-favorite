// start SkyWalking
function uuid() {
  return 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function (c) {
    /* tslint:disable */
    var r = Math.random() * 16 | 0;
    /* tslint:disable */
    var v = c === 'x' ? r : r & 0x3 | 0x8;
    return v.toString(16);
  });
}
/**
 *
 * @param {*} traceId
 * @param {*} traceSegmentId
 * @param {*} applicationId
 * @param {*} version
 * @param {*} requestUrlHost
 */
function getSkyWalkingSw8(traceId, traceSegmentId, applicationId, env, version, requestUrlHost) {
  try {
    var traceIdStr = String(Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["base64Encode"])(traceId));
