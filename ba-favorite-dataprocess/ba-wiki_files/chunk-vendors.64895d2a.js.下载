      url: entry.name,
      urlHost: urlObj.Host,
      urlPath: urlObj.Path,
      urlPathGroup: Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["replaceNumberCharByPath"])(urlObj.Path),
