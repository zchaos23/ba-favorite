/*! no static exports found */
/***/ (function(module, exports, __webpack_require__) {

var throttle = __webpack_require__(/*! ./throttle */ "./node_modules/.pnpm/throttle-debounce@1.1.0/node_modules/throttle-debounce/throttle.js");
var debounce = __webpack_require__(/*! ./debounce */ "./node_modules/.pnpm/throttle-debounce@1.1.0/node_modules/throttle-debounce/debounce.js");

module.exports = {
	throttle: throttle,
	debounce: debounce
};


