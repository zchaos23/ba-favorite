/***/ "./node_modules/.pnpm/@cloudcare+browser-rum@3.1.1/node_modules/@cloudcare/browser-rum/esm/domain/rumEventsCollection/resource/matchRequestTiming.js":
/*!***********************************************************************************************************************************************************!*\
  !*** ./node_modules/.pnpm/@cloudcare+browser-rum@3.1.1/node_modules/@cloudcare/browser-rum/esm/domain/rumEventsCollection/resource/matchRequestTiming.js ***!
