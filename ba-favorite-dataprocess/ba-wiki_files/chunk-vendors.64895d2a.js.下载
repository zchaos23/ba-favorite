  var correspondingTimingOverrides = matchingTiming ? computePerformanceEntryMetrics(matchingTiming) : undefined;
  var tracingInfo = computeRequestTracingInfo(request);
  var urlObj = Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["urlParse"])(request.url).getParse();
  var resourceEvent = Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["extend2Lev"])({
    date: startClocks.timeStamp,
    resource: {
      id: _cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["UUID"],
      type: type,
      duration: Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["msToNs"])(request.duration),
      method: request.method,
      status: request.status,
      statusGroup: Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["getStatusGroup"])(request.status),
