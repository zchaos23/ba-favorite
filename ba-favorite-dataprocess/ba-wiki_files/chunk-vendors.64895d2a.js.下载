      timestamp = Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["timeStampNow"])();
    }
    if (typeof serializationContext === 'undefined') {
      serializationContext = {
        status: _serialize__WEBPACK_IMPORTED_MODULE_3__["SerializationContextStatus"].INITIAL_FULL_SNAPSHOT,
