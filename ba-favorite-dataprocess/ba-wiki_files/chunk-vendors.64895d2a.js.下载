        event: startEvent
      }, clickActionBase);
      lifeCycle.notify(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["LifeCycleEventType"].AUTO_ACTION_COMPLETED, clickAction);
      status = ClickStatus.FINALIZED;
    },
    discard: function discard() {
      stop();
      status = ClickStatus.FINALIZED;
    }
  };
}
function finalizeClicks(clicks, rageClick) {
  var _computeFrustration = Object(_computeFrustration__WEBPACK_IMPORTED_MODULE_7__["computeFrustration"])(clicks, rageClick);
  var isRage = _computeFrustration.isRage;
  if (isRage) {
    Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["each"])(clicks, function (click) {
      click.discard();
    });
    rageClick.stop(Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["timeStampNow"])());
    rageClick.validate(Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["map"])(clicks, function (click) {
      return click.event;
    }));
  } else {
    rageClick.discard();
    Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["each"])(clicks, function (click) {
      click.validate();
    });
  }
}
