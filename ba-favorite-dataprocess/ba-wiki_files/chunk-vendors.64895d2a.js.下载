      dom_interactive: Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["toServerDuration"])(view.initialViewMetrics.domInteractive),
      error: {
        count: view.eventCounts.errorCount
      },
      first_contentful_paint: Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["toServerDuration"])(view.initialViewMetrics.firstContentfulPaint),
      first_input_delay: Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["toServerDuration"])(view.initialViewMetrics.firstInputDelay),
      first_input_time: Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["toServerDuration"])(view.initialViewMetrics.firstInputTime),
      interaction_to_next_paint: Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["toServerDuration"])(view.commonViewMetrics.interactionToNextPaint),
      is_active: view.isActive,
      name: view.name,
