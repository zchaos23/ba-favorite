function trackEventCounts(data) {
  var lifeCycle = data.lifeCycle;
  var isChildEvent = data.isChildEvent;
  var callback = data.onChange;
  if (callback === undefined) {
    callback = _cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["noop"];
  }
  var eventCounts = {
    errorCount: 0,
    longTaskCount: 0,
    resourceCount: 0,
    actionCount: 0,
    frustrationCount: 0
  };
  var subscription = lifeCycle.subscribe(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["LifeCycleEventType"].RUM_EVENT_COLLECTED, function (event) {
    if (event.type === _cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["RumEventType"].VIEW || !isChildEvent(event)) {
      return;
    }
    switch (event.type) {
      case _cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["RumEventType"].ERROR:
        eventCounts.errorCount += 1;
        callback(eventCounts);
        break;
      case _cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["RumEventType"].ACTION:
        if (event.action.frustration) {
          eventCounts.frustrationCount += event.action.frustration.type.length;
        }
        eventCounts.actionCount += 1;
        callback(eventCounts);
        break;
      case _cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["RumEventType"].LONG_TASK:
        eventCounts.longTaskCount += 1;
        callback(eventCounts);
        break;
      case _cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["RumEventType"].RESOURCE:
        eventCounts.resourceCount += 1;
        callback(eventCounts);
        break;
    }
  });
  return {
    stop: function stop() {
      subscription.unsubscribe();
    },
    eventCounts: eventCounts
  };
}
