    leading: false,
    trailing: true
  });
  var _addEventListener = Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["addEventListener"])(window, _cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["DOM_EVENT"].SCROLL, handleScrollEvent.throttled, {
    passive: true
  });
