      emit(Object(_utils__WEBPACK_IMPORTED_MODULE_6__["assembleIncrementalSnapshot"])(source, {
        positions: positions
      }));
    },
    mutationCb: mutationCb,
    scrollCb: function scrollCb(p) {
      emit(Object(_utils__WEBPACK_IMPORTED_MODULE_6__["assembleIncrementalSnapshot"])(_types__WEBPACK_IMPORTED_MODULE_2__["IncrementalSource"].Scroll, p));
    },
    styleSheetCb: function styleSheetCb(r) {
      emit(Object(_utils__WEBPACK_IMPORTED_MODULE_6__["assembleIncrementalSnapshot"])(_types__WEBPACK_IMPORTED_MODULE_2__["IncrementalSource"].StyleSheetRule, r));
