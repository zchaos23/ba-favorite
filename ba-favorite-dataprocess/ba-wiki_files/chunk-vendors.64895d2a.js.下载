  };
}
function trackFetch(lifeCycle, configuration, tracer) {
  var subscription = Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["initFetchObservable"])().subscribe(function (rawContext) {
    var context = rawContext;
    if (!Object(_rumEventsCollection_resource_resourceUtils__WEBPACK_IMPORTED_MODULE_1__["isAllowedRequestUrl"])(configuration, context.url)) {
      return;
    }
    switch (context.state) {
      case 'start':
        tracer.traceFetch(context);
        context.requestIndex = getNextRequestIndex();
