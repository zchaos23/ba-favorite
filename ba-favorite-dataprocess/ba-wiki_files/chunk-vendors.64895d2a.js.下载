/**
 * Clone input data and ensure known user properties (id, name, email)
 * are strings, as defined here:
 */
function sanitizeUser(newUser) {
  // We shallow clone only to prevent mutation of user data.
  var user = Object(_helper_tools__WEBPACK_IMPORTED_MODULE_1__["assign"])({}, newUser);
  var keys = ['id', 'name', 'email'];
  Object(_helper_tools__WEBPACK_IMPORTED_MODULE_1__["each"])(keys, function (key) {
    if (key in user) {
      user[key] = String(user[key]);
    }
  });
  return user;
}

/**
 * Simple check to ensure user is valid
 */
function checkUser(newUser) {
  var isValid = Object(_helper_tools__WEBPACK_IMPORTED_MODULE_1__["getType"])(newUser) === 'object';
  if (!isValid) {
    _helper_display__WEBPACK_IMPORTED_MODULE_0__["display"].error('Unsupported user:', newUser);
  }
