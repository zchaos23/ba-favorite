function createEventRateLimiter(eventType, limit, onLimitReached) {
  var eventCount = 0;
  var allowNextEvent = false;
  return {
    isLimitReached: function isLimitReached() {
      if (eventCount === 0) {
        Object(_timer__WEBPACK_IMPORTED_MODULE_2__["setTimeout"])(function () {
          eventCount = 0;
        }, _tools__WEBPACK_IMPORTED_MODULE_1__["ONE_MINUTE"]);
      }
      eventCount += 1;
      if (eventCount <= limit || allowNextEvent) {
        allowNextEvent = false;
        return false;
      }
      if (eventCount === limit + 1) {
        allowNextEvent = true;
        try {
          onLimitReached({
            message: 'Reached max number of ' + eventType + 's by minute: ' + limit,
            source: _errorTools__WEBPACK_IMPORTED_MODULE_0__["ErrorSource"].AGENT,
            startClocks: Object(_tools__WEBPACK_IMPORTED_MODULE_1__["clocksNow"])()
          });
        } finally {
          allowNextEvent = false;
        }
      }
      return true;
    }
  };
}
