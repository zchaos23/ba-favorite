    mediaInteractionCb: function mediaInteractionCb(p) {
      emit(Object(_utils__WEBPACK_IMPORTED_MODULE_6__["assembleIncrementalSnapshot"])(_types__WEBPACK_IMPORTED_MODULE_2__["IncrementalSource"].MediaInteraction, p));
    },
    mouseInteractionCb: function mouseInteractionCb(mouseInteractionRecord) {
      emit(mouseInteractionRecord);
    },
