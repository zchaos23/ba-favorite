function assembleIncrementalSnapshot(source, data) {
  return {
    data: Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["assign"])({
      source: source
    }, data),
    type: _types__WEBPACK_IMPORTED_MODULE_1__["RecordType"].IncrementalSnapshot,
    timestamp: Object(_cloudcare_browser_core__WEBPACK_IMPORTED_MODULE_0__["timeStampNow"])()
  };
